footer.footer
	.footer__container.container
		.footer__wrapper
			.footer__callback-wrapper
				+footerCallback(footerCallback)
				+link('footer__callback-button', 'Задать свой вопрос', '#')
			+menuFooterCategories(menuFooter)
		.footer__wrapper-info
			.footer__social-wrapper
				+list(socialLinks, 'footer__social-list', 'footer__social-item')
			.footer__copyright
				+link('footer__copyright-link', 'Пользовательское соглашение', '#')
				p.footer__copyright-text: <small>© 2022-2023 “Астраханская область”</small>

